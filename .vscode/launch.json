{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Chrome",
        "type": "pwa-chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}"
      },
      {
        "name": "Attach Chrome",
        "type": "pwa-chrome",
        "request": "attach",
        "url": "http://localhost:4200/#",
        "webRoot": "${workspaceFolder}"
      }
    ]
  }